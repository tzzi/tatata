<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="account">
	<insert id="join">
		insert into account(id,pass,nick) values(#{id},#{pass},#{nick})
	</insert>
	<select id="idcheck"  resultType="java.util.LinkedHashMap">
		select * from account where id = #{id}
	</select>
	<select id="nickcheck"  resultType="java.util.LinkedHashMap">
		select * from account where nick = #{nick}
	</select>
</mapper>